{
  "git": {
    "deploymentEnabled": {
      "main": true,
      "gh-pages": false
    }
  },
  "buildCommand": "npm run build",
  "outputDirectory": "docs/.vuepress/dist",
  "installCommand": "npm install --legacy-peer-deps",
  "devCommand": "npm run dev",
  "framework": "vuepress",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}